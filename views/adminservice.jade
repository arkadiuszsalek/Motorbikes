extends admindashboard
block content
     .row
      .holder
        h1        
        .grid-container
            each row in res1
              .item 
                h1.itemtitle #{row.Name}
                p.itemp Phone: #{row.Phone}
                p.itemp Town: #{row.Town}
                p.itemp Address: #{row.Address}
                a.button(href='editservice/#{row.Id}') Edit 
                a.button(href='deleteservice/#{row.Id}') Delete